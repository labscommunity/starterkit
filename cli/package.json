{
  "name": "starterkit",
  "version": "0.0.0",
  "description": "Create Arweave web application with starterkit",
  "author": "CommunityLabs",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/labscommunity/starterkit/issues"
  },
  "homepage": "https://github.com/labscommunity/starterkit#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/labscommunity/starterkit.git"
  },
  "keywords": [
    "starterkit",
    "walletkit",
    "arweavekit",
    "arweave",
    "next.js"
  ],
  "type": "module",
  "exports": "./dist/index.js",
  "bin": {
    "starterkit": "./dist/index.js"
  },
  "files": [
    "dist",
    "template",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "package.json"
  ],
  "engines": {
    "node": ">=14.16"
  },
  "scripts": {
    "typecheck": "tsc",
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier '**/*.{cjs,mjs,ts,tsx,md,json}' --ignore-path .gitignore --write",
    "format:check": "prettier '**/*.{cjs,mjs,ts,tsx,md,json}' --ignore-path .gitignore --check",
    "release": "bumpp && npm publish"
  },
  "dependencies": {
    "@clack/core": "^0.3.2",
    "@clack/prompts": "^0.6.3",
    "chalk": "5.2.0",
    "commander": "^10.0.0",
    "execa": "^7.1.1",
    "fs-extra": "^11.1.1",
    "gradient-string": "^2.0.2",
    "ora": "6.3.1",
    "sort-package-json": "^2.4.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/gradient-string": "^1.1.2",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.16.0",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "bumpp": "^9.2.0",
    "eslint": "^8.51.0",
    "prettier": "^3.0.0",
    "tsup": "^6.7.0",
    "type-fest": "^3.7.0",
    "typescript": "^5.0.4"
  }
}
